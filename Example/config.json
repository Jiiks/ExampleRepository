                    "options": [
                        {
                            "id": "opt1",
                            "text": "small",
                            "value": "8px"
                        },
                        {
                            "id": "opt2",
                            "text": "medium",
                            "value": "12px"
                        },
                        {
                            "id": "opt3",
                            "text": "high",
                            "value": "16px"
                        },
                        {
                            "id": "opt4",
                            "text": "ultra",
                            "value": "20px"
                        }
                    ]
                },
                {
                    "id": "radioTest",
                    "type": "radio",
                    "value": "opt1",
