                    "options": [
                        {
                            "id": "opt1",
                            "text": "red",
                            "value": "red"
                        },
                        {
                            "id": "opt2",
                            "text": "green",
                            "value": "green"
                        },
                        {
                            "id": "opt3",
                            "text": "blue",
                            "value": "blue"
                        },
                        {
                            "id": "opt4",
                            "text": "orange",
                            "value": "orange"
                        },
                        {
                            "id": "opt5",
                            "text": "white",
                            "value": "white"
                        }
                    ]
                },
                {
                    "id": "spanOpacity2",
                    "type": "number",
                    "value": 0,
                    "text": "Span opacity",
                    "hint": "Number Test Setting Hint"
                },
                {
                    "id": "default-0",
                    "type": "text",
                    "value": null,
                    "text": "Test setting #1",
